{
  "progressClasses" : {
    "type": "css-class",
    "staticClasses": ["progress-bar"],
    "optionalClasses": [
      {
        "label": "Context",
        "type" : "enum",
        "options": [
          {"label" : "None", "value" : ""},
          {"label" : "Success", "value" : "progress-bar-success"},
          {"label" : "Info", "value" : "progress-bar-info"},
          {"label" : "Warning", "value" : "progress-bar-warning"},
          {"label" : "Danger", "value" : "progress-bar-danger"}
        ]
      },
      {
        "label" : "Striped",
        "type" : "optional-string",
        "value" : "progress-bar-striped"
      },
      {
        "label" : "Animated",
        "type" : "optional-string",
        "value" : "active"
      }
    ]
  },
  "percent": {
    "label": "%",
    "type" : "number",
    "min" : 0,
    "max" : 100,
    "step" : 1
  },
  "sections": {
    "label": "Method of labeling",
    "type": "section-toggle",
    "values": [
      {
        "label": "Only for screen readers",
        "value": "default"
      },
      {
        "label": "Visually",
        "value": "label"
      }
    ]
  }
}
