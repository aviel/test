{
  "formGroupClasses": {
    "type": "attribute",
    "key": "class",
    "value": {
      "type": "group",
      "values": [
        {
          "type": "const",
          "value": "form-group"
        },
        {
          "label": "Validation",
          "type": "select",
          "optional": true,
          "values": [
            {
              "label": "Success",
              "value": "has-success"
            },
            {
              "label": "Warning",
              "value": "has-warning"
            },
            {
              "label": "Error",
              "value": "has-error"
            }
          ]
        }
      ]
    }
  },
  "inputAttrs": {
    "type": "group",
    "values": [
      {
        "type": "attribute",
        "key": "class",
        "value": {
          "type": "group",
          "values": [
            {
              "type": "const",
              "value": "form-control"
            },
            {
              "label": "Disabled",
              "type": "switch",
              "on": "disabled"
            },
            {
              "label": "Size",
              "type": "select",
              "optional": true,
              "values": [
                {
                  "label": "Large",
                  "value": "input-lg"
                },
                {
                  "label": "Small",
                  "value": "input-sm"
                }
              ]
            }
          ]
        }
      },
      {
        "label": "Read only",
        "type": "switch",
        "on": " readonly"
      }
    ]
  },
  "inputType": {
    "label": "Type",
    "type": "select",
    "values": [
      "text",
      "password",
      "datetime",
      "datetime-local",
      "date",
      "month",
      "time",
      "week",
      "number",
      "email",
      "url",
      "search",
      "tel",
      "color"
    ]
  }
}
