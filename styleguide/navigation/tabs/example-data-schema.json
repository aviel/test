{
  "navClasses": {
    "type": "css-class",
    "staticClasses": ["nav"],
    "optionalClasses": [
      {
        "label": "Tab style",
        "type": "enum",
        "values": [
          {
            "label": "Tabs",
            "value": "nav-tabs"
          },
          {
            "label": "Pills",
            "value": "nav-pills"
          }
        ]
      },
      {
        "label": "Stacked",
        "type": "optional-string",
        "value": "nav-stacked"
      },
      {
        "label": "Justified",
        "type": "optional-string",
        "value": "nav-justified"
      }
    ]
  },
  "sections": {
    "label": "Layout",
    "type": "section-toggle",
    "values": [
      {
        "label": "Basic",
        "value": "default"
      },
      {
        "label": "With dropdown",
        "value": "dropdown"
      }
    ]
  }
}
